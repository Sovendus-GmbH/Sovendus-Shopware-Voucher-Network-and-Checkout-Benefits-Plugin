{% sw_extends '@parent/storefront/page/content/index.html.twig' %}

{% block base_javascript %}
	{{ parent() }}
	{% set sovendusPageData = context.context.extensions.sovendusPageData %}
	{% if sovendusPageData %}
		<script>
			window.sovPageConfig = {{ sovendusPageData.get('jsConfig')|raw }};
		</script>
		<script src="{{ asset('bundles/sovendusplugin/storefront/js/sovendus-page.js') }}"></script>
	{% endif %}
{% endblock %}
