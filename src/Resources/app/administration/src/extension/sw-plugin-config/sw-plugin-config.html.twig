{% block sw_plugin_config %}
	{% block sw_plugin_config_sovendus %}
		<div v-if="isSovendusPlugin" class="sw-plugin-config">
			<router-link :to="pluginConfigRoute">
				<sw-button>Go to Sovendus Settings</sw-button>
			</router-link>
		</div>
	{% endblock %}

	{% block sw_plugin_config_default %}
		<div v-else class="sw-plugin-config">
			{% parent %}
		</div>
	{% endblock %}
{% endblock %}
